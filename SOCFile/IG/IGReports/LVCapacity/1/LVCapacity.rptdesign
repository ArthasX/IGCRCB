  <Script>function beforeGeneration( chart, icsc ){&#xD;
	var timeType = icsc.getExternalContext().getScriptable().getParameterValue(&quot;timeType&quot;);&#xD;
&#xD;
	//得到x轴对象&#xD;
	xAxisArray = chart.getBaseAxes();&#xD;
	//设置x轴显示为设定的时间&#xD;
	//xAxisArray[0].setCategoryAxis(false);&#xD;
	if(timeType == &quot;year&quot;){&#xD;
		xAxisArray[0].getFormatSpecifier().setPattern(&quot;yyyy/MM&quot;);&#xD;
	}else {&#xD;
		if(timeType == &quot;month&quot;) {&#xD;
			chart.getBlock().getBounds().setWidth(11.6*72);&#xD;
		}&#xD;
		xAxisArray[0].getFormatSpecifier().setPattern(&quot;yyyy/MM/dd&quot;);&#xD;
	}&#xD;
}&#xD;
&#xD;
