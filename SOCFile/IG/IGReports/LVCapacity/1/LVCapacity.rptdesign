            <method name="beforeOpen"><![CDATA[importPackage(Packages.java.io );
//out = new PrintWriter(new FileWriter("c:/info.txt", true));

var hostName = reportContext.getParameterValue("hostName");
var vgName = reportContext.getParameterValue("vgName");
var fsName = reportContext.getParameterValue("fsName");//表空间名称(柱图显示时有且只有一个值,最多12个)
var dateTime = reportContext.getParameterValue("dateTime");//日期类型值
var timeType = reportContext.getParameterValue("timeType");//month,quarter,year
var mode =reportContext.getParameterValue("mode");//many,single

var mytable;
if(timeType == "month"){
	mytable="LVCAPACITY_MONTH";
}else if(timeType == "quarter"){
	mytable="LVCAPACITY_QUARTER";
}else if(timeType == "year"){
	mytable="LVCAPACITY_YEAR";
}
var query = "SELECT HOSTNAME,VGNAME,FSNAME,TO_DATE(SUBSTR(CREATEDATE,1,10),'yyyy/mm/dd') AS CREATEDATE,ALLOTTEDCAPACITY,FREECAPACITY,USEDCAPACITY";
	query += " FROM " + mytable
	if(timeType == "quarter"){
		var datetemp = dateTime.split("/");
		if(datetemp[1] == 1){
			query += " WHERE CREATEDATE||':00' >= '" + datetemp[0] + "/01/01 00:00:00'";
			query += " AND CREATEDATE||':00' <= '" + datetemp[0] + "/03/31 23:59:59'";
		}else if(datetemp[1] == 2){
			query += " WHERE CREATEDATE||':00' >= '" + datetemp[0] + "/04/01 00:00:00'";
			query += " AND CREATEDATE||':00' <= '" + datetemp[0] + "/06/30 23:59:59'";
		}else if(datetemp[1] == 3){
			query += " WHERE CREATEDATE||':00' >= '" + datetemp[0] + "/07/01 00:00:00'";
			query += " AND CREATEDATE||':00' <= '" + datetemp[0] + "/09/31 23:59:59'";
		}else if(datetemp[1] == 4){
			query += " WHERE CREATEDATE||':00' >= '" + datetemp[0] + "/10/01 00:00:00'";
			query += " AND CREATEDATE||':00' <= '" + datetemp[0] + "/12/31 23:59:59'";
		}
	}else{
		//month和year报表需要进行模糊查询
		query += " WHERE CREATEDATE like '" + dateTime + "%'";
	}
	
	query += " AND HOSTNAME = '" + hostName + "'";
	query += " AND VGNAME = '" + vgName + "'";
	
if(mode == "single"){
	query += " AND FSNAME = '" + fsName + "'";
}else if(mode == "many"){
	var fsNameTemp = fsName.split(",");
	query += " AND FSNAME in ('" + fsNameTemp[0] + "'";
	for(var i=1;i<fsNameTemp.length;i++){
		query += ",'" + fsNameTemp[i] + "'";
	}
	query += ")";
}
//out.println(query);
//out.close();

this.queryText = query;]]></method>
